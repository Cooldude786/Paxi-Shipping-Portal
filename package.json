{
  "name": "paxi-shipper-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-dev": "ng run paxi-shipper-portal:build:development --aot --output-hashing=all",
